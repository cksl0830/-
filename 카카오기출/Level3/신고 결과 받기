def solution(id_list, report, k):
    
    answer = {x: 0 for x in id_list}  # 메일 받는 횟수를 value값으로
    recieve = {x: 0 for x in id_list}  # 신고 받은 횟수를 value값으로

    for s in set(report):
        recieve[s.split()[1]] += 1

    for i in set(report):
        if recieve[i.split()[1]] >= k:
            answer[i.split()[0]] += 1
    
    return list(answer.values())

     
