def solution(array, commands):
    answer = []
    sortlist=[]

    for i in commands:
        sortlist=array[i[0]-1: i[1]]
        sortlist.sort()
        answer.append(sortlist[i[2]-1])

    return answer
    
    
// 다른사람의 풀이 

def solution(array, commands):
  return list(map(lambda x:sorted(array[x[0]-1:x[1]])[x[2]-1], commands))
